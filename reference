@ARTICLE{Lin2020-hl,
  title    = "Fatty Acid Oxidation Controls {CD8+} {Tissue-Resident} Memory
              T-cell Survival in Gastric Adenocarcinoma",
  author   = "Lin, Run and Zhang, Hui and Yuan, Yujie and He, Qiong and Zhou,
              Jianwen and Li, Shuhua and Sun, Yu and Li, Daniel Y and Qiu,
