@ARTICLE{Barsch2022-ef,
  title    = "T-cell exhaustion and residency dynamics inform clinical outcomes
              in hepatocellular carcinoma",
  author   = "Barsch, Maryam and Sali{\'e}, Henrike and Schlaak, Alexandra
              Emilia and Zhang, Zhen and Hess, Moritz and Mayer, Lena Sophie
              and Tauber, Catrin and Otto-Mora, Patricia and Ohtani, Takuya and
              Nilsson, Tobias and Wischer, Lara and Winkler, Frances and Manne,
              Sasikant and Rech, Andrew and Schmitt-Graeff, Annette and
