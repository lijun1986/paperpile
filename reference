@ARTICLE{Attrill2022-vr,
  title    = "Higher proportions of {CD39+} tumor-resident cytotoxic {T} cells
              predict recurrence-free survival in patients with stage {III}
              melanoma treated with adjuvant immunotherapy",
  author   = "Attrill, Grace Heloise and Owen, Carina N and Ahmed, Tasnia and
              Vergara, Ismael A and Colebatch, Andrew J and Conway, Jordan W
              and Nahar, Kazi J and Thompson, John F and Pires da Silva, Ines
