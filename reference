  number    =  0,
  month     =  may,
  year      =  2022,
  keywords  = "XIST; X chromosome; transcriptional reactivation; polycomb;
               human mammary stem cell; homeostasis; differentiation; Mediator;
               enhancers; breast tumorigenesis",
